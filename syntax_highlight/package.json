{
  "name": "ufrgscript-syntax",
  "displayName": "UFRGScript Syntax Highlighting",
  "description": "Syntax highlighting for UFRGScript",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "themes": [
      {
        "label": "UFRGScript Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/ufrgscript-color-theme.json"
      }
    ],
    "languages": [
      {
        "id": "ufrgscript",
        "aliases": [
          "UFRGScript",
          "ufrgscript"
        ],
        "extensions": [
          ".ufrgs"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ufrgscript",
        "scopeName": "source.ufrgscript",
        "path": "./syntaxes/ufrgscript.tmLanguage.json"
      }
    ]
  },
  "main": "./none.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
